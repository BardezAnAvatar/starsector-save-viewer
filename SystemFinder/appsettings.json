{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": {
      "FileSink": {
        "Name": "File",
        "Args": { "path": "log_{timestamp}.log" }
      }
    },
    "Properties": {
      "Application": "Bardez.Starsector.SystemViewer"
    }
  }
}
